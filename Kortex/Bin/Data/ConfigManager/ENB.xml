<?xml version="1.0" encoding="utf-8"?>
<Config>
	<Categories>
		<Entry Label="$T(GameConfig.Categories.ENB.General)">ENB.General</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Effects)">ENB.Effects</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Bloom)">ENB.Bloom</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.AmbientOcclusion)">ENB.AO</Entry>
		<Entry Label="$T(GameConfig.Categories.Shadows)">ENB.Shadows</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.FPSLimit)">ENB.FPSLimit</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.ProxyLibrary)">ENB.ProxyLibrary</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Controls)">ENB.Controls</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Fixes)">ENB.Fixes</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Antialiasing)">ENB.Antialiasing</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Performance)">ENB.Performance</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Memory)">ENB.Memory</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.AdaptiveQuality)">ENB.AdaptiveQuality</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Multihead)">ENB.Multihead</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.ColorCorrection)">ENB.ColorCorrection</Entry>
		<Entry Label="$T(GameConfig.Categories.ENB.Weather)">ENB.Weather</Entry>
		<Entry Label="$T(GameConfig.Categories.Misc)">ENB.Misc</Entry>
	</Categories>

	<Data>
		<File ID="-1000">
			<FormatOptions>
				<float>.2</float>
				<bool>s</bool>
			</FormatOptions>
			
			<Values>
				<!-- General -->
				<Entry Category="ENB.General" Path="ENGINE" Name="ForceAnisotropicFiltering" Type="bool"></Entry>
				<Entry Category="ENB.General" Path="GLOBAL" Name="UseDefferedRendering" Type="bool"></Entry>
				<Entry Category="ENB.General" Path="WINDOW" Name="ForceBorderless" Type="bool"></Entry>
				<Entry Category="ENB.General" Path="WINDOW" Name="ForceBorderlessFullscreen" Type="bool"></Entry>
				<Entry Category="ENB.General" Path="ENGINE" Name="EnableVSync" Type="bool"></Entry>
				<Entry Category="ENB.General" Path="ENGINE" Name="VSyncSkipNumFrames" Type="int32" Editable="0">
					<SampleValues Min="0" Max="3" Step="1"></SampleValues>
				</Entry>
				<Entry Category="ENB.General" Path="ENGINE" Name="MaxAnisotropy" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.None)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">4</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">8</Entry>
						<Entry Label="$T(ConfigManager.Samples.Ultra)">12</Entry>
						<Entry Label="$T(ConfigManager.Samples.Extreme)">16</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.General" Path="ENGINE" Name="AddDisplaySuperSamplingResolutions" Type="bool"></Entry>
				
				<!-- Effects -->
				<Entry Category="ENB.Effects" Path="GLOBAL" Name="UseEffect" Type="bool"></Entry>

				<!-- FPS Limit -->
				<Entry Category="ENB.FPSLimit" Path="LIMITER" Name="EnableFPSLimit" Type="bool"></Entry>
				<Entry Category="ENB.FPSLimit" Path="LIMITER" Name="WaitBusyRenderer" Type="bool"></Entry>
				<Entry Category="ENB.FPSLimit" Path="LIMITER" Name="FPSLimit" Type="float32">
					<SampleValues Min="0" Max="60" Step="10"></SampleValues>
				</Entry>

				<!-- ProxyLibrary -->
				<Entry Category="ENB.ProxyLibrary" Path="PROXY" Name="EnableProxyLibrary" Type="bool"></Entry>
				<Entry Category="ENB.ProxyLibrary" Path="PROXY" Name="InitProxyFunctions" Type="bool"></Entry>
				<Entry Category="ENB.ProxyLibrary" Path="PROXY" Name="ProxyLibrary" Type="string"></Entry>

				<!-- Controls -->
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyReadConfig" Type="int32" SubType="VirtualKey" Editable="1">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyCombination" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyUseEffect" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyFPSLimit" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyShowFPS" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyScreenshot" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyEditor" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyFreeVRAM" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>
				<Entry Category="ENB.Controls" Path="INPUT" Name="KeyBruteForce" Type="int32" SubType="VirtualKey">
					<SampleValues FillFunction="GetVirtualKeys"></SampleValues>
				</Entry>

				<!-- Fixes -->
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixPhysics" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixGameBugs" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixParallaxBugs" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixAliasedTextures" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="IgnoreLoadingScreen" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="IgnoreInventory" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixSsaoWaterTransparency" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixSsaoHairTransparency" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixTintGamma" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="RemoveBlur" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixSubSurfaceScattering" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixParallaxTerrain" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixSkyReflection" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixCursorVisibility" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="FIX" Name="FixLag" Type="bool"></Entry>
				<Entry Category="ENB.Fixes" Path="GLOBAL" Name="IgnoreCreationKit" Type="bool"></Entry>

				<!-- Antialiasing -->
				<Entry Category="ENB.Antialiasing" Path="ANTIALIASING" Name="EnableEdgeAA" Type="bool"></Entry>
				<Entry Category="ENB.Antialiasing" Path="ANTIALIASING" Name="EnableSubPixelAA" Type="bool"></Entry>
				<Entry Category="ENB.Antialiasing" Path="ANTIALIASING" Name="EnableTemporalAA" Type="bool"></Entry>
				
				<!-- Performance -->
				<Entry Category="ENB.Performance" Path="GLOBAL" Name="UsePatchSpeedhackWithoutGraphics" Type="bool"></Entry>
				<Entry Category="ENB.Performance" Path="PERFORMANCE" Name="SpeedHack" Type="bool"></Entry>
				<Entry Category="ENB.Performance" Path="PERFORMANCE" Name="EnableOcclusionCulling" Type="bool"></Entry>
				<Entry Category="ENB.Performance" Path="THREADS" Name="DataSyncMode" Type="int32" Editable="0">
					<SampleValues Min="0" Max="2" Step="1"></SampleValues>
				</Entry>
				<Entry Category="ENB.Performance" Path="THREADS" Name="PriorityMode" Type="int32" Editable="0">
					<SampleValues Min="0" Max="4" Step="1"></SampleValues>
				</Entry>
				
				<!-- Memory -->
				<Entry Category="ENB.Memory" Path="MEMORY" Name="ExpandSystemMemoryX64" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="ReduceSystemMemoryUsage" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="DisableDriverMemoryManager" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="DisablePreloadToVRAM" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="ReservedMemorySizeMb" Type="uint32">
					<SampleValues>
						<Entry>64</Entry>
						<Entry>128</Entry>
						<Entry>256</Entry>
						<Entry>384</Entry>
						<Entry>512</Entry>
						<Entry>640</Entry>
						<Entry>768</Entry>
						<Entry>1024</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="VideoMemorySizeMb" Type="uint32" Editable="1">
					<SampleValues Min="0" Max="16384" Step="1024"></SampleValues>
				</Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="EnableCompression" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="AutodetectVideoMemorySize" Type="bool"></Entry>
				<Entry Category="ENB.Memory" Path="MEMORY" Name="EnableUnsafeMemoryHacks" Type="bool"></Entry>

				<!-- Adaptive Quality -->
				<Entry Category="ENB.AdaptiveQuality" Path="ADAPTIVEQUALITY" Name="Enable" Label="$T(GameConfig.Values.EnableAdaptiveQuality)" Type="bool"></Entry>
				<Entry Category="ENB.AdaptiveQuality" Path="ADAPTIVEQUALITY" Name="Quality" Label="$T(GameConfig.Values.AdaptiveQuality)" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AdaptiveQuality" Path="ADAPTIVEQUALITY" Name="DesiredFPS" Label="$T(GameConfig.Values.AdaptiveQualityDesiredFPS)" Type="float32">
					<SampleValues Min="0" Max="60" Step="10"></SampleValues>
				</Entry>

				<!-- Multihead -->
				<Entry Category="ENB.Multihead" Path="MULTIHEAD" Name="ForceVideoAdapterIndex" Type="bool"></Entry>
				<Entry Category="ENB.Multihead" Path="MULTIHEAD" Name="VideoAdapterIndex" Type="uint32"></Entry>

				<!-- Misc -->
				<Entry Category="ENB.Misc" Path="ENGINE" Name="ForceLodBias" Type="bool"></Entry>
				<Entry Category="ENB.Misc" Path="ENGINE" Name="LodBias" Type="int32" Editable="0">
					<SampleValues Min="-4" Max="4" Step="1"></SampleValues>
				</Entry>
			</Values>
		</File>
		
		<File ID="-1001">
			<FormatOptions>
				<float>.2</float>
				<bool>s</bool>
			</FormatOptions>
			
			<Values>
				<!-- Effects -->
				<Entry Category="ENB.Effects" Path="EFFECT" Name="UseOriginalPostProcessing" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="UseOriginalObjectsProcessing" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableAdaptation" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableDepthOfField" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableDetailedShadow" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableSunRays" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableSkyLighting" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableImageBasedLighting" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableReflection" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableSoftParticles" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableParticleLights" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableSunGlare" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableSubSurfaceScattering" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableLens" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableWater" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableUnderwater" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableCloudShadows" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableVolumetricRays" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableProceduralSun" Type="bool"></Entry>
				<Entry Category="ENB.Effects" Path="EFFECT" Name="EnableMist" Type="bool"></Entry>

				<!-- Bloom -->
				<Entry Category="ENB.Bloom" Path="EFFECT" Name="EnableBloom" Type="bool"></Entry>
				<Entry Category="ENB.Bloom" Path="BLOOM" Name="Quality" Label="$T(GameConfig.Values.BloomQuality)" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
					</SampleValues>
				</Entry>

				<!-- Ambient Occlusion -->
				<Entry Category="ENB.AO" Path="EFFECT" Name="EnableAmbientOcclusion" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="UseIndirectLighting" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="UseComplexIndirectLighting" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="UseComplexAmbientOcclusion" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="UseAmbientIndirectLighting" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="EnableDenoiser" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="EnableSupersampling" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="EnableComplexFilter" Type="bool"></Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="SamplingQuality" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Ultra)">-1</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="SamplingPrecision" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Ultra)">-1</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="FilterQuality" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.VeryLow)">-1</Entry>
						<Entry Label="$T(ConfigManager.Samples.Low)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">2</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="FilterType" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.VeryLow)">4</Entry>
						<Entry Label="$T(ConfigManager.Samples.Low)">3</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.Ultra)">0</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="AOType" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">2</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="ILType" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">2</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="AOMixingType" Type="int32" Editable="0">
					<SampleValues Min="0" Max="7" Step="1"></SampleValues>
				</Entry>
				<Entry Category="ENB.AO" Path="SSAO_SSIL" Name="AOMixingTypeInterior" Type="int32" Editable="0">
					<SampleValues Min="0" Max="7" Step="1"></SampleValues>
				</Entry>

				<!-- Shadows -->
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="ShadowFilterQuality" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="DetailedShadowQuality" Type="int32">
					<SampleValues>
						<Entry Label="$T(ConfigManager.Samples.Low)">2</Entry>
						<Entry Label="$T(ConfigManager.Samples.Mid)">1</Entry>
						<Entry Label="$T(ConfigManager.Samples.High)">0</Entry>
						<Entry Label="$T(ConfigManager.Samples.Ultra)">-1</Entry>
					</SampleValues>
				</Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="UseBilateralShadowFilter" Type="bool"></Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="UseShadowFilter" Type="bool"></Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="ShadowCastersFix" Type="bool"></Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="ShadowQualityFix" Type="bool"></Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="ShadowBlurRange" Type="float32">
					<SampleValues Min="0.1" Max="8" Step="1"></SampleValues>
				</Entry>
				<Entry Category="ENB.Shadows" Path="SHADOW" Name="ShadowBlurRangeInterior" Type="float32">
					<SampleValues Min="0.1" Max="8" Step="1"></SampleValues>
				</Entry>

				<!-- ColorCorrection -->
				<Entry Category="ENB.ColorCorrection" Path="COLORCORRECTION" Name="UsePaletteTexture" Type="bool"></Entry>
				<Entry Category="ENB.ColorCorrection" Path="COLORCORRECTION" Name="UseProceduralCorrection" Type="bool"></Entry>
				<Entry Category="ENB.ColorCorrection" Path="COLORCORRECTION" Name="Brightness" Type="float32">
					<SampleValues Min="0" Max="5" Step="0.5"></SampleValues>
				</Entry>
				<Entry Category="ENB.ColorCorrection" Path="COLORCORRECTION" Name="GammaCurve" Type="float32">
					<SampleValues Min="1" Max="2.5" Step="0.5"></SampleValues>
				</Entry>
				
				<!-- Weather -->
				<Entry Category="ENB.Weather" Path="WEATHER" Name="EnableMultipleWeathers" Type="bool"></Entry>
				<Entry Category="ENB.Weather" Path="WEATHER" Name="EnableLocationWeather" Type="bool"></Entry>
				
				<!-- Misc -->
			</Values>
		</File>
	</Data>
</Config>
