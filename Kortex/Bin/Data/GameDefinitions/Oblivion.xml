				<Entry Label="$T(SaveManager.Filter.Backups) (*.bak)">*.bak</Entry>
				<Entry Label="$T(SaveManager.Filter.CoSaves) (*.obse)">*.obse</Entry>
			</FileFilters>

			<PrimaryExtension>ess</PrimaryExtension>
			<SecondaryExtension>obse</SecondaryExtension>
		</SaveManager>
